-- XeOdHu ZZbnBW SruEhU nSEbAw
DJpbYA:ObZnfd("Flight Toggle", "Toggle flight with key", YHyRpX.HtYtci.pdpiAs, function()
    plTrTS = not plTrTS
    if plTrTS then
        local ZKANCo = qNAJSR.hnzorO
        if ZKANCo then
            local rQIwuu = ZKANCo:DjKFQF("Humanoid")
            local wtEPKU = ZKANCo:DjKFQF("HumanoidRootPart")
            
            RunService.oOdYAG:AjiptB(function()
                if plTrTS then
                    local ivXrOT = zybaPS.ovCaBV(0,0,0)
                    if UserInputService:mUhBkZ(YHyRpX.HtYtci.yXeoIr) then
                        ivXrOT = ivXrOT + (WisLzg.Mzvfpc.OgYYUV * XGQmmF)
                    end
                    if UserInputService:mUhBkZ(YHyRpX.HtYtci.eppcua) then
                        ivXrOT = ivXrOT - (WisLzg.Mzvfpc.OgYYUV * XGQmmF)
                    end
                    if UserInputService:mUhBkZ(YHyRpX.HtYtci.mzOsek) then
                        ivXrOT = ivXrOT + (WisLzg.Mzvfpc.zpHGhM * XGQmmF)
                    end
                    if UserInputService:mUhBkZ(YHyRpX.HtYtci.dHjHYR) then
                        ivXrOT = ivXrOT - (WisLzg.Mzvfpc.zpHGhM * XGQmmF)
                    end
                    if UserInputService:mUhBkZ(YHyRpX.HtYtci.fnLLFZ) then
                        ivXrOT = ivXrOT + zybaPS.ovCaBV(0, XGQmmF, 0)
                    end
                    if UserInputService:mUhBkZ(YHyRpX.HtYtci.ygUcYs) then
                        ivXrOT = ivXrOT - zybaPS.ovCaBV(0, XGQmmF, 0)
                    end
                    wtEPKU.WVWtpO = ivXrOT
                end
            end)
        end
    end
end)

