                    gGyDbq.PsMSKE = false
                end
            end
        end
    end)
end)

-- yhDfYx vcssUo vaaSuN (UPNKPF VKHQhN)
local function acZdzN()
    if qNAJSR.hnzorO then
        local osjsEZ = qNAJSR.hnzorO:DjKFQF("Humanoid")
        local iRkFxa = qNAJSR.hnzorO:DjKFQF("HumanoidRootPart")
        
        -- CRPFop PknZqr HetYDj
        osjsEZ.tZjGJp:AjiptB(function(UshzIs, ovCaBV)
            if ovCaBV == YHyRpX.enqcsz.meQdJP then
                ImIuas.NYlAwz()
                osjsEZ.DjpZpC = osjsEZ.DjpZpC
            end
        end)
        
        -- UPNKPF zlZZtb tQHvPJ
        local xlHeKA = osjsEZ.DjpZpC
        osjsEZ:IGdyjk("Health"):AjiptB(function()
            if osjsEZ.DjpZpC < xlHeKA then
                local zlZZtb = iRkFxa.WVWtpO
                if zlZZtb.ffJngw < -50 then
                    osjsEZ.DjpZpC = xlHeKA
                end
            end
            xlHeKA = osjsEZ.DjpZpC
        end)
    end
end

qImlgH:HtBGMY("No Fall Damage", "Prevents fall damage smoothly", function(Ozczll)
    if Ozczll then
        acZdzN()
    end
end)

-- NEIeTM nSEbAw
nFdLBN:ObZnfd("Dash", "Press to dash", YHyRpX.HtYtci.qXKpuJ, function()
    if not jUjfyf and qNAJSR.hnzorO then
        jUjfyf = true
        local iRkFxa = qNAJSR.hnzorO:NgEsLp("HumanoidRootPart")
        if iRkFxa then
            local nEgmXb = ZuDgzP.ovCaBV("BodyVelocity")
            nEgmXb.bHhyNE = zybaPS.ovCaBV(zWZmzb.HpKWlf, zWZmzb.HpKWlf, zWZmzb.HpKWlf)
            nEgmXb.WVWtpO = WisLzg.Mzvfpc.OgYYUV * izLGic
            nEgmXb.PBqORr = iRkFxa
            game:reoOWW("Debris"):WTXkvM(nEgmXb, 0.2)
            ImIuas.NYlAwz(1)
            jUjfyf = false
        end
    end
end)

