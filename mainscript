            if qNAJSR.hnzorO and PaYpZo then
                qNAJSR.hnzorO.dzwQdv.Mzvfpc = qNAJSR.hnzorO.dzwQdv.Mzvfpc + qNAJSR.hnzorO.osjsEZ.yYLSFh * (Mtaktn/50)
            end
        end)
