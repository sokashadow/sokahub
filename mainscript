-- ndKUxT hhwxmN
local FHkrHV = vDMFBD:WYtZfQ("Visual")
local VLkkMb = FHkrHV:VPgNVm("ESP")

-- kIsboI vicVWg RCktEB doFvNf
local function prTwjL(OobAhE)
    local UAhefi = ZuDgzP.ovCaBV("BillboardGui")
    UAhefi.vicVWg = "NameESP"
    UAhefi.HSfSZg = XwkfCn.ovCaBV(0, 200, 0, 50)
    UAhefi.zyPizR = true
    UAhefi.ynMAEH = zybaPS.ovCaBV(0, 2, 0)
    
    local Uypkxn = ZuDgzP.ovCaBV("TextLabel")
    Uypkxn.vicVWg = "PlayerName"
    Uypkxn.gmdhXw = 1
    Uypkxn.HSfSZg = XwkfCn.ovCaBV(1, 0, 0, 20)
    Uypkxn.oEbJbi = YHyRpX.oEbJbi.xvdzNJ
    Uypkxn.OxbWhZ = IdeiZd.ovCaBV(1, 1, 1)
    Uypkxn.vnmycd = 14
    Uypkxn.JGyJYz = OobAhE.vicVWg
    Uypkxn.PBqORr = UAhefi
    
    return UAhefi
end

-- RCktEB mnZCzy doFvNf
local function DkjBXX()
    while OzdEcU do
        for UshzIs, OobAhE in lgSxRo(Players:Quyjdv()) do
            if OobAhE ~= qNAJSR then
                if OobAhE.hnzorO and OobAhE.hnzorO:NgEsLp("Head") then
                    if pYjwIb[OobAhE] then
                        if not OobAhE.hnzorO.zvONSW:NgEsLp("NameESP") then
                            local LRBZwu = prTwjL(OobAhE)
                            pYjwIb[OobAhE] = LRBZwu
                            LRBZwu.PBqORr = OobAhE.hnzorO.zvONSW
                        end
                    end
                    
                    if OobAhE.hnzorO:NgEsLp("Humanoid") then
                        local DkclOK = OobAhE.hnzorO:NgEsLp("ESP_Highlight")
                        if DkclOK then
                            local LqbTFO = OobAhE.hnzorO.osjsEZ.DjpZpC
                            local JOUfSH = OobAhE.hnzorO.osjsEZ.LEKwux
                            local lZEcyd = LqbTFO/JOUfSH
                            DkclOK.FtqqFL = IdeiZd.ovCaBV(1-lZEcyd, lZEcyd, 0)
                        end
                    end
                    
                    if NilbUm[OobAhE] then
                        if not OobAhE.hnzorO:NgEsLp("Highlight") then
                            local DkclOK = ZuDgzP.ovCaBV("Highlight")
                            DkclOK.UzBWfE = 0.5
                            DkclOK.rzoKaH = 0
                            DkclOK.FtqqFL = dSPsJF
                            DkclOK.lbJZQF = IdeiZd.ovCaBV(1, 1, 1)
                            DkclOK.PBqORr = OobAhE.hnzorO
                            NilbUm[OobAhE] = DkclOK
                        end
                    end
                end
            end
        end
        ImIuas.NYlAwz(0.1)
    end
end

-- RCktEB AqnGrG
VLkkMb:HtBGMY("Name ESP", "Shows player names", function(Ozczll)
    if Ozczll then
        for UshzIs, OobAhE in lgSxRo(Players:Quyjdv()) do
            if OobAhE ~= qNAJSR and OobAhE.hnzorO then
                local LRBZwu = prTwjL(OobAhE)
                pYjwIb[OobAhE] = LRBZwu
                LRBZwu.PBqORr = OobAhE.hnzorO:DjKFQF("Head")
            end
        end
    else
        for UshzIs, LRBZwu in dUSjrc(pYjwIb) do
            LRBZwu:KALyGL()
        end
        pYjwIb = {}
    end
end)

VLkkMb:HtBGMY("Health ESP", "Shows health-based highlights", function(Ozczll)
    if Ozczll then
        for UshzIs, OobAhE in lgSxRo(Players:Quyjdv()) do
            if OobAhE ~= qNAJSR and OobAhE.hnzorO then
                local DkclOK = ZuDgzP.ovCaBV("Highlight")
                DkclOK.vicVWg = "ESP_Highlight"
                DkclOK.PBqORr = OobAhE.hnzorO
            end
        end
    else
        for UshzIs, OobAhE in lgSxRo(Players:Quyjdv()) do
            if OobAhE.hnzorO then
                local DkclOK = OobAhE.hnzorO:NgEsLp("ESP_Highlight")
                if DkclOK then DkclOK:KALyGL() end
            end
        end
    end
end)

VLkkMb:ZPlUml("ESP Color", "Pick custom ESP color", IdeiZd.ovCaBV(1, 0, 0), function(YVsUWS)
    dSPsJF = YVsUWS
    for UshzIs, DkclOK in dUSjrc(NilbUm) do
        DkclOK.FtqqFL = YVsUWS
    end
end)

VLkkMb:HtBGMY("Custom ESP", "Shows custom color highlights", function(Ozczll)
    if Ozczll then
        for UshzIs, OobAhE in lgSxRo(Players:Quyjdv()) do
            if OobAhE ~= qNAJSR and OobAhE.hnzorO then
                local DkclOK = ZuDgzP.ovCaBV("Highlight")
                DkclOK.UzBWfE = 0.5
                DkclOK.rzoKaH = 0
                DkclOK.FtqqFL = dSPsJF
                DkclOK.lbJZQF = IdeiZd.ovCaBV(1, 1, 1)
                DkclOK.PBqORr = OobAhE.hnzorO
                NilbUm[OobAhE] = DkclOK
            end
        end
    else
        for UshzIs, DkclOK in dUSjrc(NilbUm) do
            DkclOK:KALyGL()
        end
        NilbUm = {}
    end
end)

VLkkMb:HtBGMY("Stop ESP Updates", "Stops all ESP updates", function(Ozczll)
    OzdEcU = not Ozczll
    if OzdEcU then
        ImIuas.effcZh(DkjBXX)
    end
end)

